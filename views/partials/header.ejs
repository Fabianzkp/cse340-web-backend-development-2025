<header id="top-header">
  <span class="siteName">
    <a href="/" title="Return to home page">CSE Motors</a>
  </span>
  <div id="tools">
    <% if (loggedIn) { %>
     <a href="/" title="Go to account management" class="account">Welcome <%= accountData.account_firstname %> |</a>
      <a href="/account/logout" class="log" title="Click to log out">| Logout</a>
    <% } else { %>
      <a href="/account/login" class="log" title="Click to log in">Login</a>
    <% } %>
  </div>
</header>